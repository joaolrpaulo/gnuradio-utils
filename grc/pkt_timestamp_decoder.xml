<?xml version="1.0"?>
<block>
  <name>Messages Decoder</name>
  <key>pkt_timestamp_decoder</key>
  <category>Indoor Location</category>
  <import>import pkt</import>
  <make>pkt.timestamp_decoder(id=$identifier)</make>
  <param>
    <name>ID</name>
    <key>identifier</key>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
</block>
